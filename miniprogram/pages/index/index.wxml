<swiper
 autoplay="{{true}}"
 interval="{{interval}}"
 circular="{{true}}"
 duration="{{duration}}"
 indicator-dots="{{true}}"
 indicator-active-color="#fff"
 indicator-active-color="#8ECDA9"
>
	<block wx:for="{{imgUrls}}" wx:key="{{index}}">
		<swiper-item>
			<image class="slide-image" src="{{item}}"></image>
		</swiper-item>
	</block>
</swiper>

<view class="main">
	<view class="main-enterUniversity">
		<view class="main-title">
			{{enterUniversity}}
		</view>
		<view class="main-funcIcon">
			<view
			 class="main-iconDetail"
			 wx:for="{{enterUniversityList}}"
			 wx:key="index"
			 bindtap="getFunction"
			 data-id="{{item.id}}"
			>
				<view class="image">
					<image class="funcImage" src="{{item.imgUrl}}"></image>
				</view>
				<text>{{item.text}}</text>
			</view>

		</view>
	</view>
	<view class="main-enterUniversity">
		<view class="main-title">
			{{universityServer}}
		</view>
		<view class="main-funcIcon">
			<view
			 class="main-iconDetail"
			 wx:for="{{universityServerList}}"
			 wx:key="index"
			>
				<view class="image">
					<image class="funcImage" src="{{item.imgUrl}}"></image>
				</view>
				<text>{{item.text}}</text>
			</view>
		</view>
	</view>
</view>

